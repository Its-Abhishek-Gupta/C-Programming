//WAP that tells whether a given year is leap year or not
#include<stdio.h>
int main()
{
    int year;
    printf("Enter the year to find whether it is leap or not:");
    scanf("%d",&year);
    if (year%400==0 || year%4==0){
        printf("\nThe given year is a leap year");
    }
    else{
        printf("\nThe given year is not a leap year");
    }
    return 0;
}
