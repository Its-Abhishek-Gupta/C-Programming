//WAP to swap values of two variables using third variable
#include<stdio.h>
int main()
{
    int a,b,c;
    printf("Enter two numbers a and b to swap:");
    scanf("%d%d",&a,&b);
    printf("\nBefore swapping a =%d, b=%d",a,b);
    c=a;
    a=b;
    b=c;
    printf("\nAfter swapping a=%d,b=%d",a,b);
    return 0;
}
