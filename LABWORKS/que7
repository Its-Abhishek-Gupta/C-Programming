//WAP to find the greatest of three numbers
#include<stdio.h>
int main()
{
    int num1,num2,num3;
    printf("Input three numbers num1,num2 and num3 respectively:");
    scanf("%d%d%d",&num1,&num2,&num3);
    if (num1>num2 && num1>num3){
        printf("The greatest number is=%d",num1);
    }
    else if (num2>num1 && num2>num3){
        printf("The greatest numberr is=%d",num2);
    }
    else{
        printf("The greatest number is=%d",num3);
    }
    return 0;
}
