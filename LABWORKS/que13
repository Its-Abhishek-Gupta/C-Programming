// WAP to find the factorial of a given number.
#include<stdio.h>
int main()
{
    int number,i,factorial;
    printf("Enter the number to find the factorial:");
    scanf("%d",&number);
    i=1;
    factorial=1;
    while (i<=number){
        factorial=factorial*i;
        i=i+1;
    }
    printf("Factorial of the given number is=%d",factorial);
    return 0;
}
