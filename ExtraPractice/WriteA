//WAP to write A using stars
#include<stdio.h>
int main()
{
    for (int i=0; i<=10; i=i+1)
    {
        for (int j=0 ;j<=10; j=j+1)
        {
            if (i==0 || i==5 || j==10 || j==0){
                printf("*");
            }
            else{
                printf(" ");
            }
        }
        printf("\n");
    }
    return 0;
}
